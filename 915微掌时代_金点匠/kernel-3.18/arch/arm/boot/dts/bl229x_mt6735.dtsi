&pio{
	fingerprint_spi0_default:spi0_def{
		pins_cmd_dat {
			pins = <PINMUX_GPIO9__FUNC_SPI3_CS>;
			slew-rate = <1>;
			bias-disable;
		};
		pins_cmd2_dat {
			pins = <PINMUX_GPIO10__FUNC_SPI3_SCK>;
			slew-rate = <1>;
			bias-disable;
		};
		pins_cmd3_dat {
			pins = <PINMUX_GPIO12__FUNC_SPI3_MISO>;
			slew-rate = <0>;
			bias-disable;
		};
		pins_cmd4_dat {
			pins = <PINMUX_GPIO11__FUNC_SPI3_MOSI>;
			slew-rate = <1>;
			bias-disable;
		};
	};
	fingerprint_rst_default:fingerprint_rst_def{
		pins_cmd_dat {
			pins = <PINMUX_GPIO20__FUNC_GPIO20>;
			slew-rate = <1>;
			output-high;
		};
	};
	fingerprint_rst_disable:fingerprint_rst_dis{
		pins_cmd_dat {
			pins = <PINMUX_GPIO20__FUNC_GPIO20>;
			slew-rate = <1>;
			output-low;
		};
	};

	fingerprint_power_en_default:fingerprint_power_en_def{
		pins_cmd_dat {
			pins = <PINMUX_GPIO14__FUNC_GPIO14>;
			slew-rate = <1>;
			output-high;
		};
	};
	fingerprint_power_en_disable:fingerprint_power_en_dis{
		pins_cmd_dat {
			pins = <PINMUX_GPIO14__FUNC_GPIO14>;
			slew-rate = <1>;
			output-low;
		};
	};

	fingerprint_int_default:fingerprint_int_def{
		pins_cmd_dat {
			pins = <PINMUX_GPIO15__FUNC_EINT15>;
			slew-rate = <0>;
			bias-pull-down = <00>;
		};
	};
};

&spi0{
	#address-cells = <1>;
	#size-cells = <0>;
	fingerprint@0{
		compatible = "blestech,BL229X";
		
		reg = <0x0>;
		spi-max-frequency = <0x10000000>;
		pinctrl-names = "spi0_default", "rst_output1", "rst_output0", "power_en_output1", "power_en_output0" ,"int_default";
		pinctrl-0 = <&fingerprint_spi0_default>;
		pinctrl-1 = <&fingerprint_rst_default>;
		pinctrl-2 = <&fingerprint_rst_disable>;
		pinctrl-3 = <&fingerprint_power_en_default>;
		pinctrl-4 = <&fingerprint_power_en_disable>;
		pinctrl-5 = <&fingerprint_int_default>;
		
		fingerprint,rst-gpio = <&pio 20 0>;
		fingerprint,touch-int-gpio = <&pio 15 0>;
		fingerprint,en-gpio = <&pio 14 0>;
		
		interrupt-parent = <&eintc>;
		interrupts = <15 IRQ_TYPE_LEVEL_HIGH>;
		debounce = <3 0>;
		
		status = "okay";
	};
};

/*
#define IRQ_TYPE_NONE		0
#define IRQ_TYPE_EDGE_RISING	1
#define IRQ_TYPE_EDGE_FALLING	2
#define IRQ_TYPE_EDGE_BOTH	(IRQ_TYPE_EDGE_FALLING | IRQ_TYPE_EDGE_RISING)
#define IRQ_TYPE_LEVEL_HIGH	4
#define IRQ_TYPE_LEVEL_LOW	8
*/

